<robot name="fw">
    <link name="base_link">
        <visual>
            <geometry>
                <box size="0.5 0.2 0.1" />
            </geometry>
            <origin xyz="0 0 0.1" rpy="0 0 0" />  
            <material name="white">
                <color rgba="0.6 0.6 0.6 1"/>
            </material>
        </visual>
        <visual>
            <geometry>
                <box size="0.2 0.2 0.1" />
            </geometry>
            <origin xyz="0.144 0 0.144" rpy="0 0.5 0" />  
            
        </visual>
        <visual>
            <geometry>
                <box size="0.35 0.2 0.1" />
            </geometry>
            <origin xyz="-0.08 0 0.18" rpy="0 0 0" />  
            
        </visual>
        <visual>
            <!-- 此处的origin的xyz是指相对于base而言的， -->
            <origin xyz=" -0.05 0 0.25" rpy="0 0 0" />
            <geometry>
                <cylinder length="0.1" radius="0.075"/>
            </geometry>
            <material name="black">
                <color rgba="0.1 0.1 0.1 1"/>
            </material>
        </visual>
    </link>

    <joint name="base_left_motor_joint" type="fixed">
        <origin xyz="-0.16 -0.11 0.05" rpy="0 0 0" />        
        <parent link="base_link"/>
        <child link="left_f_motor"/>
    </joint>

     <link name="left_f_motor">
        <visual>
            <origin xyz="0 0 0" rpy="1.5707 0 0"/>
            <geometry>
                <sphere radius="0.0165" />
            </geometry>
            <material name="black">
                <color rgba="0 0 0 0.95"/>
            </material>
        </visual>
    </link>

    <joint name="base_right_motor_joint" type="fixed">
        <origin xyz="-0.16 0.11 0.05" rpy="0 0 0" />        
        <parent link="base_link"/>
        <child link="right_f_motor"/>
    </joint>

     <link name="right_f_motor">
        <visual>
            <origin xyz="0 0 0" rpy="1.5707 0 0"/>
            <geometry>
                <sphere radius="0.0165" />
            </geometry>
            <material name="black">
                <color rgba="0 0 0 0.95"/>
            </material>
        </visual>
    </link>

    <joint name="base_right_b_motor_joint" type="fixed">
        <origin xyz="0.16 0.11 0.05" rpy="0 0 0" />        
        <parent link="base_link"/>
        <child link="right_b_f_motor"/>
    </joint>

     <link name="right_b_f_motor">
        <visual>
            <origin xyz="0 0 0 " rpy="1.5707 0 0"/>
            <geometry>
                <sphere radius="0.0165" />
            </geometry>
            <material name="black">
                <color rgba="0 0 0 0.95"/>
            </material>
        </visual>
    </link>

    <joint name="base_left_b_motor_joint" type="fixed">
        <origin xyz="0.16 -0.11 0.05" rpy="0 0 0" />        
        <parent link="base_link"/>
        <child link="left_b_f_motor"/>
    </joint>

     <link name="left_b_f_motor">
        <visual>
            <origin xyz="0 0 0" rpy="1.5707 0 0"/>
            <geometry>
                <sphere radius="0.0165" />
            </geometry>
            <material name="black">
                <color rgba="0 0 0 0.95"/>
            </material>
        </visual>
    </link>

    <joint name="left_wheel_joint" type="continuous">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="left_f_motor"/>
        <child link="left_wheel_link"/>
        <axis xyz="0 1 0"/>
    </joint>

    <link name="left_wheel_link">
        <visual>
            <origin xyz="0 0 0" rpy="1.5707 0 0" />
            <geometry>
                <cylinder radius="0.05" length = "0.017"/>
            </geometry>
            <material name="grey">
                <color rgba="0.2 0.2 0.2 1"/>
            </material>
        </visual>
    </link>

    <joint name="right_wheel_joint" type="continuous">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="right_f_motor"/>
        <child link="right_wheel_link"/>
        <axis xyz="0 1 0"/>
        
    </joint>

    <link name="right_wheel_link">
        <visual>
            <origin xyz="0 0 0" rpy="1.5707 0 0" />
            <geometry>
                <cylinder radius="0.05" length = "0.017"/>
            </geometry>
            <material name="grey">
                <color rgba="0.2 0.2 0.2 1"/>
            </material>
        </visual>
    </link>

    <joint name="b_right_wheel_joint" type="continuous">
        <origin xyz="0 0 0 " rpy="0 0 0"/>
        <parent link="right_b_f_motor"/>
        <child link="b_right_wheel_link"/>
        <axis xyz="0 1 0"/>
    </joint>

    <link name="b_right_wheel_link">
        <visual>
            <origin xyz="0 0 0" rpy="1.5707 0 0" />
            <geometry>
                <cylinder radius="0.05" length = "0.017"/>
            </geometry>
            <material name="grey">
                <color rgba="0.2 0.2 0.2 1"/>
            </material>
        </visual>
    </link>


    <joint name="b_left_wheel_joint" type="continuous">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="left_b_f_motor"/>
        <child link="b_left_wheel_link"/>
        <axis xyz="0 1 0"/>
    </joint>

    <link name="b_left_wheel_link">
        <visual>
            <origin xyz="0 0 0" rpy="1.5707 0 0" />
            <geometry>
                <cylinder radius="0.05" length = "0.017"/>
            </geometry>
            <material name="grey">
                <color rgba="0.2 0.2 0.2 1"/>
            </material>
        </visual>
    </link>

    

</robot>
